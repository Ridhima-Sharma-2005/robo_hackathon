



<launch>
<!-- Launch Your robot arms launch file which loads the robot in Gazebo and spawns the
controllers -->
<!-- Launch Moveit Move Group Node -->
<include file = "$(find robot_description)/launch/arm_robot.launch" />
<include file = "$(find ronotic_arm_moveit)/launch/move_group.launch" />
<!-- Run Rviz and load the default configuration to see the state of the move_group node -->
<arg name="use_rviz" default="true" />
<include file="$(find ronotic_arm_moveit)/launch/moveit_rviz.launch" if="$(arg
use_rviz)">
<arg name="rviz_config" value="$(find ronotic_arm_moveit)/launch/moveit.rviz"/>
</include>
</launch>
